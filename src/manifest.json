{
    "name": "DnTpbHider",
    "version": "0.5.1",
    "manifest_version": 2,
    "description": "Hide torrents from a list view",
    "default_locale": "en",
    "homepage_url": "https://github.com/MacDada/DnTpbHider",
    "author": "Dawid Nowak",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "http://thepiratebay.org/*",
                "https://thepiratebay.org/*",
                "http://thepiratebay.se/*",
                "https://thepiratebay.se/*",
                "http://thepiratebay.gy/*",
                "https://thepiratebay.gy/*",
                "http://thepiratebay.ac/*",
                "https://thepiratebay.ac/*",
                "http://thepiratebay.gd/*",
                "https://thepiratebay.gd/*",
                "http://thepiratebay.gs/*",
                "https://thepiratebay.gs/*",
                "http://thepiratebay.am/*",
                "https://thepiratebay.am/*",
                "http://thepiratebay.mn/*",
                "https://thepiratebay.mn/*",
                "http://thepiratebay.la/*",
                "https://thepiratebay.la/*",
                "http://thepiratebay.vg/*",
                "https://thepiratebay.vg/*"
            ],
            "css": [
                "style.css"
            ],
            "js": [
                "jquery-2.1.0.min.js",
                "jquery.sortElements-0.11.js",
                "script.js"
            ]
        }
    ]
}
